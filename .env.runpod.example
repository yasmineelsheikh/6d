# RunPod GPU Configuration (optional - for remote Cosmos execution)
RUNPOD_API_KEY=your_runpod_api_key_here
RUNPOD_ENDPOINT_ID=your_endpoint_id  # Optional: auto-created if not specified
COSMOS_EXECUTION_MODE=auto  # Options: 'local', 'runpod', 'auto' (default: auto)

# Cloud Storage for RunPod (required for RunPod mode)
# Option 1: AWS S3
AWS_ACCESS_KEY_ID=your_aws_key
AWS_SECRET_ACCESS_KEY=your_aws_secret
S3_BUCKET_NAME=cosmos-temp-storage


